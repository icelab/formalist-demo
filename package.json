{
  "name": "formalist-demo",
  "description": "Asset compilation with webpack",
  "version": "1.0.0",
  "author": "Icelab",
  "repository": {},
  "dependencies": {
    "domready": "1.0.8",
    "formalist-compose": "icelab/formalist-compose",
